<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="PrimaryBrush" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Offset="0" Color="{DynamicResource PrimaryColor}"/>
        <GradientStop Offset="1" Color="{DynamicResource DarkPrimaryColor}"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="DangerBrush" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Offset="0" Color="{DynamicResource DangerColor}"/>
        <GradientStop Offset="1" Color="{DynamicResource DarkDangerColor}"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="WarningBrush" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Offset="0" Color="{DynamicResource WarningColor}"/>
        <GradientStop Offset="1" Color="{DynamicResource DarkWarningColor}"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="MouseHoverBrush" Color="{DynamicResource PrimaryColor}"/>
    <SolidColorBrush x:Key="MouseDownBrush" Color="{DynamicResource DarkPrimaryColor}"/>
</ResourceDictionary>